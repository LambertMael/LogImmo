<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="403.0" prefWidth="424.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.logimmo.AddEquipementController">
    <children>
        <ListView fx:id="photos" layoutX="40.0" layoutY="138.0" prefHeight="200.0" prefWidth="200.0" />
        <Label fx:id="labelPhotos" layoutX="107.0" layoutY="104.0" text="Photos" />
        <Button fx:id="btnPa" layoutX="271.0" layoutY="204.0" mnemonicParsing="false" onAction="#addPhoto" text="Ajouter une photo" />
        <Button fx:id="btnPd" layoutX="263.0" layoutY="162.0" mnemonicParsing="false" onAction="#deletePhoto" text="Supprimer une photo" />
        <Button layoutX="275.0" layoutY="48.0" mnemonicParsing="false" onAction="#save" prefHeight="64.0" prefWidth="107.0" text="Enregistrer" />
        <TextField fx:id="libelle" layoutX="66.0" layoutY="55.0" promptText="Libelle (Ex : Frigo)" />
      <Label layoutX="108.0" layoutY="31.0" text="Libelle" />
    </children>
</AnchorPane>
